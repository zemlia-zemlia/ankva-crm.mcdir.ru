{
  "name": "execut/yii2-actions",
  "description": "A standard set of actions for yii2 through TDD ",
  "keywords": ["yii2 actions", "yii2 action"],
  "homepage": "https://github.com/execut/yii2-actions",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "eXeCUT"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "execut/yii2-base": "*",
    "kartik-v/yii2-export": "*",
    "kartik-v/yii2-dynagrid": "*",
    "kartik-v/yii2-detail-view": "*",
    "kartik-v/yii2-mpdf": "*",
    "kartik-v/yii2-widget-alert": "*",
    "kartik-v/yii2-date-range": "*",
    "unclead/yii2-multiple-input": "*",
    "execut/yii2-loading-overlay": "*",
    "execut/yii2-kcfinder": "*"
  },
  "require-dev": {
    "codeception/codeception": "@dev"
  },
  "autoload": {
    "psr-4": {
      "execut\\actions\\": ""
    }
  },
  "scripts": {
    "test": "./vendor/bin/codecept run --no-interaction"
  }
}